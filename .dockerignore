# Ignore vendor (will be installed during build)
vendor/

# Ignore node_modules
node_modules/

# Ignore local storage files (will use R2 cloud storage)
storage/app/public/*
!storage/app/public/.gitignore

# Ignore local environment files
.env
.env.backup
.env.local

# Ignore IDE files
.idea/
.vscode/
*.swp
*.swo

# Ignore logs
storage/logs/*
!storage/logs/.gitignore

# Ignore cache
storage/framework/cache/*
!storage/framework/cache/.gitignore
storage/framework/sessions/*
!storage/framework/sessions/.gitignore
storage/framework/views/*
!storage/framework/views/.gitignore

# Ignore OS files
.DS_Store
Thumbs.db
